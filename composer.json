{
    "name": "brunofalcaodev/eduka",
    "description": "Eduka - Standalone course platform for my courses",
    "keywords": [
        "brunofalcaodev",
        "eduka",
        "laravel"
    ],
    "homepage": "https://github.com/brunofalcaodev/eduka",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.0",
        "laravel/nova": "^3.30",
        "illuminate/support": "^8.0",
        "doctrine/dbal": "^2.9",
        "spatie/laravel-medialibrary": "^9.8"
    },
    "autoload": {
        "psr-4": {
            "Eduka\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eduka\\EdukaServiceProvider"
            ]
        }
    }
}
