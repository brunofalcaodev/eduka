{
    "name": "brunofalcaodev/eduka",
    "description": "Eduka - Standalone course platform for my courses",
    "keywords": [
        "brunofalcaodev",
        "eduka",
        "laravel"
    ],
    "homepage": "https://github.com/brunofalcaodev/eduka",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.0",
        "brunocfalcao/helpers": "dev-master",
        "doctrine/dbal": "^2.9",
        "illuminate/support": "^8.0",
        "intervention/image": "^2.7",
        "predis/predis": "^1.1",
        "gliterd/laravel-backblaze-b2": "^1.1",
        "spatie/laravel-medialibrary": "^9.8",
        "protonemedia/laravel-paddle": "^2.2",
        "spatie/laravel-activitylog": "^4.3",
        "spatie/laravel-honeypot": "^4.0",
        "wildbit/swiftmailer-postmark": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Eduka\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eduka\\EdukaServiceProvider"
            ]
        }
    }
}
